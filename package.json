{
  "name": "wix-react-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for react projects using webpack 2 (based on https://brotzky.co/blog/code-splitting-react-router-webpack-2/)",
  "main": "index.js",
  "author": "Greg Wixted <gwixted@gmail.com> (but really it's https://brotzky.co/)",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development node webpack/webpack-dev-server --env.dev",
    "build": "rm -Rf build/* | NODE_ENV=production webpack --config webpack/webpack.config.js --progress --env.prod"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "immutability-helper": "^2.1.2",
    "mobx": "^3.1.7",
    "mobx-localstorage": "^0.1.7",
    "mobx-react": "^4.1.3",
    "open-browser-webpack-plugin": "^0.0.5",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "html-webpack-plugin": "^2.28.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
